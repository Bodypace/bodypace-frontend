{
  "name": "bodypace-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write '(src|stories)/**/**.(tsx|ts|css)'",
    "format:check": "prettier --check '(src|stories)/**/**.(tsx|ts|css)'",
    "test": "test-storybook",
