    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "chromatic": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "msw": "^2.2.0",
    "msw-storybook-addon": "^2.0.0-beta.1",
    "postcss": "^8",
    "prettier": "3.1.1",
    "storybook": "^7.6.4",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
